@model IEnumerable<Online_game.Models.ViewModel.BiddingHistoryViewModel>

@{
    ViewBag.Title = "Bidding";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Bidding</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaxAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GameName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaxAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GameName)
        </td>
    </tr>
}

</table>
